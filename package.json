{
  "name": "regexgen-cli",
  "description": "Generates regular expressions that match a set of strings.",
  "homepage": "https://github.com/Kikobeats/regexgen-cli",
  "version": "1.1.17",
  "main": "index.js",
  "bin": {
    "regexgen": "bin/index.js"
  },
  "author": {
    "email": "josefrancisco.verdu@gmail.com",
    "name": "Kiko Beats",
    "url": "https://github.com/Kikobeats"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kikobeats/regexgen-cli.git"
  },
  "bugs": {
    "url": "https://github.com/Kikobeats/regexgen-cli/issues"
  },
  "keywords": [
    "cli",
    "regexgen"
  ],
  "dependencies": {
    "get-stdin": "~8.0.0",
    "meow": "~13.1.0",
    "regexgen": "~1.3.0",
    "strip-final-newline": "~2.0.0",
    "update-notifier": "~5.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "ci-publish": "latest",
    "conventional-github-releaser": "latest",
    "coveralls": "latest",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "git-dirty": "latest",
    "simple-git-hooks": "latest",
    "nano-staged": "latest",
    "mocha": "latest",
    "npm-check-updates": "latest",
    "nyc": "latest",
    "should": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "standard-version": "latest"
  },
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "bin"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "standard-markdown && standard",
    "postrelease": "npm run release:tags && npm run release:github && ci-publish",
    "prerelease": "npm run update:check",
    "pretest": "npm run lint",
    "release": "git-authors-cli && git add package.json && standard-version -a",
    "release:github": "conventional-github-releaser -p angular",
    "release:tags": "git push --follow-tags origin HEAD:master",
    "test": "nyc mocha",
    "update": "ncu -a",
    "update:check": "ncu -- --error-level 2"
  },
  "preferGlobal": true,
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "simple-git-hooks": {
  "commit-msg": "npx commitlint --edit",
  "pre-commit": "npx nano-staged"
},
  "nano-staged": {
    "package.json": [
      "finepack",
      "git add"
    ]
  }
}
